{
    "name": "ipa-server",
    "description": "Deploy ipa-server on Heroku.",
    "keywords": [
        "ipa-server"
    ],
    "env": {
        "PUBLIC_URL": {
            "description": "public URL for this server, empty to use $DOMAIN",
            "required": false,
            "value": ""
        },
        "QINIU": {
            "description": "qiniu config AK:SK:[ZONE]:BUCKET",
            "required": false,
            "value": ""
        },
        "QINIU_URL": {
            "description": "qiniu bucket public url, https://cdn.example.com",
            "required": false,
            "value": ""
        },
        "ALIOSS": {
            "description": "alicloud OSS config ENDPOINT:ID:SECRET:BUCKET",
            "required": false,
            "value": ""
        },
        "ALIOSS_URL": {
            "description": "alicloud OSS bucket public url, https://cdn.example.com",
            "required": false,
            "value": ""
        },
        "DELETE_ENABLED": {
            "description": "delete app enabled, true/false",
            "required": false,
            "value": "true"
        },
        "META_PATH": {
            "description": "option, metadata storage path, use random secret path to keep your metadata safer in case of remote storage",
            "required": false,
            "value": "appList.json"
        }
    },
    "website": "https://github.com/iineva/ipa-server",
    "repository": "https://github.com/iineva/ipa-server",
    "stack": "container"
}